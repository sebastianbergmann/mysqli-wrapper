{
    "name": "sebastian/mysqli-wrapper",
    "description": "Wrapper around PHP's mysqli extension",
    "type": "library",
    "keywords": ["mysql", "mysqli", "mariadb"],
    "homepage": "https://github.com/sebastianbergmann/mysqli-wrapper",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/sebastianbergmann/mysqli-wrapper/issues"
    },
    "require": {
        "php": ">=8.3",
        "ext-mysqli": "*",
        "sebastian/csv-parser": "^1.0"
    },
    "config": {
        "platform": {
            "php": "8.3.9"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
